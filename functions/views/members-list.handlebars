  <script>
    $(document).ready(function () {

      var xmlhttp = new XMLHttpRequest();
      xmlhttp.open("GET", "https://us-central1-reib-platform.cloudfunctions.net/app/api/Members", true);
      xmlhttp.onreadystatechange = function () {
        if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
          var persons = JSON.parse(xmlhttp.responseText);
          $("#example").DataTable({
            data: persons,
            "columns": [
              { "data": "reacNo" },
              { "data": "name" },
              { "data": "surname" },
              { "data": "email" },
              { "data": "phoneNumber" },
              { "data": "status" },
              { "data": "typeOfWork" },
              { "data": "postalAddress" },
              { "data": "organization" },

            ]
          });
        }
      };
      xmlhttp.send();
    });
  </script>


